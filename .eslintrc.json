{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
        "airbnb-typescript",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": ["tsconfig.json"]
	},
	"plugins": ["react"],
	"rules": {
		"react/react-in-jsx-scope": "off",
        "@typescript-eslint/triple-slash-reference": "off"
	}
}
